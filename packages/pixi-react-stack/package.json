    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@pixi/layout": "catalog:pixi",
    "@pixi/react": "catalog:pixi",
    "@pixi/text": "catalog:pixi",
    "@pixi/tilemap": "catalog:pixi",
    "lucide": "^0.542.0",
    "pixi-viewport": "catalog:pixi",
    "pixi.js": "catalog:pixi",
    "use-deep-compare": "^1.3.0"
  },
  "devDependencies": {
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@vitejs/plugin-react": "^5.0.2",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "typescript": "catalog:",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
